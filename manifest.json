{
    "name": "Hide Html Contents",
    "description": "Hite html contents",
    "omnibox": {
      "keyword": "dapi"
          },
    "icons": {
      "16": "ico.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
     "browser_action": {      
      "default_popup": "popup.html",
      "default_title": "Click here!"
    },
    "content_scripts": [
    {
        "matches": ["http://openload.co/*", "https://openload.co/*", "http://*.javseen.com/*",
         "http://*.javhd.com/*", "http://*.jav789.com/*", "http://*.javstream365.com/*", "http://*.javsfind.com/*", "http://*.javpub.me/*",
         "http://*.javpub.com/*",
       "http://*.motherless.com/*",
       "http://*.prwidgets.com/*",
       "http://*.javur.com/*", "*://*/*"],

        "js": ["script/jquery-1.12.4.min.js","script/script.js"],
        "css" : ["css/style.css", "css/font-awesome.min.css"]
    }],
    "permissions": [ "webNavigation", "*://*/*" ],
    "version": "1.0",
    "minimum_chrome_version": "9",
    "manifest_version": 2
  }


  